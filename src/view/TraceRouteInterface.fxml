<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bp" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewController">
   <top>
      <HBox prefHeight="0.0" prefWidth="1917.0" spacing="4.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="800.0" text="IP Address" />
            <TextField fx:id="ipAddress" alignment="CENTER" prefHeight="25.0" prefWidth="800.0" />
            <Button fx:id="generateRandomIP" alignment="CENTER" mnemonicParsing="false" onAction="#generateRandomIpButton" prefWidth="800.0" text="Generate Random Ip" />
            <Button fx:id="traceRoute" alignment="CENTER" mnemonicParsing="false" onAction="#traceRouteButton" prefWidth="800.0" text="Trace It" />
         </children>
      </HBox>
   </top>
</BorderPane>
